{
    "name": "muyaedward/messenger",
    "description": "Simple user messaging tool for Laravel",
    "keywords": ["laravel", "messenger", "messaging", "user messaging"],
    "license": "MIT",
    "authors": [
        {
            "name": "Edward Mwangi",
            "email": "muyaedward@gmail.com",
            "homepage": "https://github.com/muyaedward",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.5.0",
        "illuminate/config": "^5.5|^6.*",
        "illuminate/support": "^5.5|^6.*",
        "illuminate/database": "^5.5|^6.*",
        "nesbot/carbon": "^1.0|^2.0"
    },
    "require-dev": {
        "adamwathan/faktory": "0.3.*",
        "friendsofphp/php-cs-fixer": "^2.5",
        "orchestra/testbench": "~3.0",
        "phpunit/phpunit": "7.*|8.*"
    },
    "autoload": {
        "psr-4": {
            "Muyaedward\\Messenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Muyaedward\\Messenger\\Test\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Muyaedward\\Messenger\\MessengerServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "stable"
}
